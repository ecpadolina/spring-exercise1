<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="personDaoImpl" class="ecp.spring.dao.PersonDaoImpl" autowire="byName"/>
  <bean id="roleDaoImpl" class="ecp.spring.dao.RoleDaoImpl" autowire="byName"/>
  <bean id="projectDaoImpl" class="ecp.spring.dao.ProjectDaoImpl" autowire="byName"/>
</beans>
